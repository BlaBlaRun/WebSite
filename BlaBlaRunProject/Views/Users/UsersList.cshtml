<table id="tableUsers" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th *ngFor="let columnName of columnNames">{{columnName}}</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td class="last-status" colspan="3">{{lastStatus}}</td>
        </tr>
    </tfoot>
    <tbody>
        <tr *ngFor="let user of list">
            <td style="display:none;">
                {{user.Id}}
            </td>
            <td>
                {{user.AspNetUserId}}
            </td>
            <td>
                {{user.UserName}}
            </td>
        </tr>
    </tbody>
</table> 